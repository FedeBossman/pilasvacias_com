{
  "vars": {
    "@brand-primary": "#1D1D1D",
    "@body-bg": "@brand-primary",
    "@text-color": "#FFF",
    "@link-color": "#e44d26",
    "@link-hover-color": "darken(@link-color, 18%)",
    "@code-bg": "#282828",
    "@navbar-default-color": "#FFF",
    "@navbar-default-bg": "#1D1D1D",
    "@navbar-default-border": "darken(@navbar-default-bg, 7%)",
